{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ]
    }
}